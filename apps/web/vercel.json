{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "cd ../.. && npx pnpm@9.15.0 install --no-frozen-lockfile",
  "buildCommand": "cd ../.. && npx pnpm@9.15.0 --filter=@repo/web... build",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api-puce-delta.vercel.app/api/:path*"
    }
  ]
}
